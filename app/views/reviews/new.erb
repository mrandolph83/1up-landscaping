<%= render partial: 'layouts/errors', locals: {object: @review}  %>
<h1> Add a Review for (Job Title) </h1>
<%= form_for(@review) do |f| %>

    <%= f.label :rating %>
    <%= f.number_field :rating, min:0, max:5 %><br>

    <%= f.label :description %> <br>
    <%= f.text_area :description, size: "25x10" %><br>

    <%= f.label :job_id %> 
    <%= f.hidden_field :job_id %>

    <%= f.submit "Sign Up" %>
    <% end %>

    